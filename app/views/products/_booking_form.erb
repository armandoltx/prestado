

<%= form_for [@product, Booking.new] do |f|%>
    <div class="col-xs-12 col-md-4 product-new-profile">
      <div>
        <%= f.label :start_time %>
        <%= f.text_field :start_time, class:"datepicker" %>
      </div>
      <div>
        <%= f.label :end_time %>
        <%= f.text_field :end_time, class:"datepicker" %>

    </div><!-- end product-profile -->
    <%= f.submit 'submit', :class => 'submit' %>
<% end %>
